{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "globals": {
    "expect": true,
    "sinon": true,
    "fetch": true,
    "window": true,
    "$": true
  }
}