{
  "name": "webpack-es2017-babel-mocha-example",
  "version": "0.0.1",
  "description": "Node babel template for the sake of nuclear chickens.",
  "main": "build/client.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "prebuild": "npm run clean && eslint src/ && npm run test",
    "build": "webpack -p",
    "start": "node index.js",
    "pretest": "eslint test/",
    "test": "mocha --use_strict --require babel-polyfill --compilers js:babel-register test/**/*.spec.js"
  },
  "author": "Andras Popovics",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "mocha-cli": "^1.0.1",
    "webpack": "^2.2.0-rc.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "csvtojson": "^1.1.4",
    "express": "^4.14.1",
    "winston": "^2.3.1"
  }
}
